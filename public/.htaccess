<IfModule mod_rewrite.c>
  RewriteEngine On
  RewriteBase /

  # Allow API routes through if they exist on this domain.
  RewriteRule ^api/ - [L]

  # Serve the SPA entry for all non-file, non-directory routes.
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteRule ^ index.html [L]
</IfModule>
